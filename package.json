{
  "name": "ntnyq.com",
  "version": "0.0.1",
  "private": true,
  "description": "ntnyq.com",
  "keywords": [
    "ntnyq"
  ],
  "bugs": {
    "url": "https://github.com/ntnyq/ntnyq.com/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ntnyq/ntnyq.com"
  },
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "serve": "gatsby serve",
    "start": "npm run develop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "package.json": "sort-package-json"
  },
  "prettier": "@ntnyq/prettier-config",
  "eslintConfig": {
    "extends": [
      "@ntnyq/prettier"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "extends": "@ntnyq/prettier-typescript-react"
      }
    ],
    "root": true
  },
  "dependencies": {
    "gatsby": "^2.29.2",
    "gatsby-image": "^2.8.0",
    "gatsby-plugin-google-analytics": "^2.8.0",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-manifest": "^2.9.1",
    "gatsby-plugin-offline": "^3.7.1",
    "gatsby-plugin-react-helmet": "^3.7.0",
    "gatsby-plugin-sharp": "^2.11.2",
    "gatsby-plugin-styled-components": "^3.7.0",
    "gatsby-plugin-typescript": "^2.9.0",
    "gatsby-source-filesystem": "^2.8.1",
    "gatsby-transformer-json": "^2.8.0",
    "gatsby-transformer-sharp": "^2.9.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.2.1",
    "styled-reset": "^4.3.4"
  },
  "devDependencies": {
    "@ntnyq/eslint-config-prettier-typescript-react": "^0.1.12",
    "@ntnyq/prettier-config": "^0.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.0.0",
    "@types/scrollreveal": "^0.0.6",
    "@types/styled-components": "^5.1.7",
    "dotenv": "^8.2.0",
    "eslint": "^7.16.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "sort-package-json": "^1.48.0",
    "typescript": "^4.1.3"
  }
}