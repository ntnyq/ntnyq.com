  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "serve": "gatsby serve",
    "start": "npm run develop"
  },
  "lint-staged": {
    "*.md": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "package.json": "sort-package-json"
  },
  "prettier": "@ntnyq/prettier-config",
  "eslintConfig": {
    "extends": [
      "@ntnyq/prettier"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "extends": "@ntnyq/prettier-typescript-react"
      }
    ],
    "root": true
  },
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "gatsby": "^3.7.2",
    "gatsby-image": "^3.7.1",
    "gatsby-plugin-google-analytics": "^3.7.1",
    "gatsby-plugin-graphql-codegen": "^3.0.0",
    "gatsby-plugin-manifest": "^3.7.1",
    "gatsby-plugin-offline": "^4.7.1",
    "gatsby-plugin-react-helmet": "^4.7.1",
    "gatsby-plugin-sharp": "^3.7.1",
    "gatsby-plugin-styled-components": "^4.7.1",
    "gatsby-plugin-typescript": "^3.7.1",
    "gatsby-source-filesystem": "^3.7.1",
    "gatsby-transformer-json": "^3.7.1",
    "gatsby-transformer-sharp": "^3.7.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-jinke-music-player": "^4.24.1",
    "styled-components": "^5.3.0",
    "styled-reset": "^4.3.4"
  },
  "devDependencies": {
    "@ntnyq/eslint-config-prettier-typescript-react": "^0.1.14",
    "@ntnyq/prettier-config": "^0.3.0",
    "@types/loadable__component": "^5.13.3",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-helmet": "^6.1.1",
    "@types/scrollreveal": "^0.0.6",
    "@types/styled-components": "^5.1.10",
    "dotenv": "^10.0.0",
    "eslint": "^7.29.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "sort-package-json": "^1.50.0",
    "typescript": "^4.3.4"
  },
  "resolution": {
    "xstate": "4.9.1"
  }
